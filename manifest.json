{
   "content_scripts": [ {
      "js": [ "mtf.js" ],
      "matches": [ "*://*.tetrisfriends.com/games/*/game.php*" ],
      "run_at": "document_end"
   } ],
   "description": "Reduces lag as much as possible by removing everything from the page except for the game itself.",
   "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "48": "logo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Minimal Tetris Friends",
   "permissions": [ "tabs", "*://*.tetrisfriends.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.4"
}
